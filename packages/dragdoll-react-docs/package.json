{
  "name": "dragdoll-react-docs",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vitepress dev docs --host",
    "build": "vitepress build docs",
    "build:examples": "node ./scripts/build-examples.mjs",
    "serve": "vitepress serve docs",
    "prettier:check": "prettier --check --no-error-on-unmatched-pattern \"./**/*.{ts,tsx,js,jsx,mjs,cjs,json,md,html,css,yml,yaml}\"",
    "prettier:write": "prettier --write --no-error-on-unmatched-pattern \"./**/*.{ts,tsx,js,jsx,mjs,cjs,json,md,html,css,yml,yaml}\"",
    "ts:check": "tsc --noEmit"
  },
  "dependencies": {
    "dragdoll": "file:../dragdoll",
    "dragdoll-react": "file:../dragdoll-react",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2"
  }
}
